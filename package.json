{
  "name": "cornerstone-app",
  "version": "2.2.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "start": "node dist/server.js",
    "start:dev": "NODE_ENV=development nodemon dist/server.js",
    "start:prod": "NODE_ENV=production nodemon dist/server.js",
    "watch": "concurrently -n 'tsc,alias' -c 'yellow,magenta'  --kill-others \"tsc -w\" \"tsc-alias -w\"",
    "watch:js": "parcel watch ./public/js/index.js --out-dir ./public/js --public-url /js --out-file bundle.js",
    "build:js": "parcel build ./p ublic/js/index.js --out-dir ./public/js --public-url /js --out-file bundle.js",
    "client": "cd client && npm start",
    "server": "nodemon dist/server.js",
    "dev": "concurrently -n 'server,client' -c 'green,blue' \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client && npm run build",
    "build": "tsc ; tsc-alias ; cp -r ./src/graphql/typedefs ./dist/graphql/typedefs"
  },
